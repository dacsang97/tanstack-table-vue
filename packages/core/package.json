{
  "name": "tanstack-table-vue",
  "version": "0.0.9",
  "homepage": "https://github.com/dacsang97/tanstack-table-vue",
  "bugs": {
    "url": "https://github.com/dacsang97/tanstack-table-vue/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dacsang97/tanstack-table-vue.git"
  },
  "files": [
    "./dist"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.mts",
      "import": "./dist/plugins/index.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "vue-tsc --noEmit",
    "pub:release": "pnpm publish --no-git-checks --access public"
  },
  "dependencies": {
    "@tanstack/vue-table": "^8.21.3",
    "lodash.capitalize": "^4.2.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/lodash.capitalize": "^4.2.9",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "latest",
    "@vue/tsconfig": "^0.8.1",
    "happy-dom": "latest",
    "tsdown": "^0.20.0-beta.3",
    "typescript": "~5.8.0",
    "unplugin-vue": "latest",
    "vitest": "latest",
    "vue-tsc": "^2.2.4"
  }
}